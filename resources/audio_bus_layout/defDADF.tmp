[sub_resource type="GDScript" id="GDScript_enlbn"]
script/source = "extends AudioEffectFilter

const TWEEN_DURATION := 0.05
const CUTOFF_HZ_MAX := 20500

var cutoff_hz_initial := cutoff_hz

func tween_create(final_val: float) -> Tween:
	
	var tween = Globals.tree.create_tween()
	tween.tween_property(self, \"cutoff_hz\", final_val, TWEEN_DURATION)
	
	return tween

func tween_in() -> void:
	return tween_create(cutoff_hz_initial)

func tween_out() -> void:
	return tween_create(CUTOFF_HZ_MAX)
"

