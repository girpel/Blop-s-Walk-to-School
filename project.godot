; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Blop's Walk to School"
run/main_scene="res://scenes/game/game.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://textures/boot_splash.png"
boot_splash/use_filter=false
config/icon="res://textures/icon.png"

[audio]

buses/default_bus_layout="res://resources/audio_bus_layout/default.tres"

[autoload]

Globals="*res://scripts/autoloads/globals.gd"
Inputs="*res://scripts/autoloads/inputs.gd"
Options="*res://scripts/autoloads/options.gd"
UIAudioStreamPlayer="*res://scripts/autoloads/ui_audio_stream_player.gd"

[display]

window/size/viewport_width=512
window/size/viewport_height=288
window/size/window_width_override=1024
window/size/window_height_override=576
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[editor]

movie_writer/movie_file="D:\\User\\Downloads\\capture.avi"
version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/chunk_texture_importer/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "red",
"res://fonts/": "orange",
"res://resources/": "yellow",
"res://scenes/": "blue",
"res://scripts/": "green",
"res://shaders/": "teal",
"res://streams/": "pink",
"res://textures/": "purple"
}

[filesystem]

import/blender/enabled=false

[gui]

theme/custom="res://resources/themes/default.tres"
theme/custom_font="res://fonts/pixel_font.ttf"
theme/default_font_antialiasing=0
theme/default_font_subpixel_positioning=0

